#include <stdio.h>
int main() {
    int a[5] = {10, 20, 30, 40, 50}, key, low = 0, high = 4, mid, found = 0;
    printf("Enter element to search: ");
    scanf("%d", &key);
    while(low <= high) {
        mid = (low + high) / 2;
        if(a[mid] == key) {
            printf("Element found at index %d\n", mid);
            found = 1;
            break;
        } else if(a[mid] < key)
            low = mid + 1;
        else
            high = mid - 1;
    }
    if(!found)
        printf("Element not found\n");
    return 0;
}

