#include <stdio.h>
int main() {
    int a[5] = {11, 22, 33, 44, 55}, i, key;
    printf("Enter element: ");
    scanf("%d", &key);
    for(i = 0; i < 5; i++) {
        if(a[i] == key) {
            printf("Element found at position %d\n", i + 1);
            return 0;
        }
    }
    printf("Element not found\n");
    return 0;
}
