#include <stdio.h>
int main() {
    char str[50];
    printf("Enter a string: ");
    scanf("%s", str);
    for(int i = 0; str[i] != '\0'; i++) {
        for(int j = i + 1; str[j] != '\0'; j++) {
            if(str[i] == str[j]) {
                printf("Character '%c' repeated at index %d\n", str[i], j);
            }
        }
    }
    return 0;
}
